	"nearbyRoad":
	[
	"PREFIX geo: <http://www.opengis.net/ont/geosparql#> ",
	"PREFIX geof: <http://www.opengis.net/def/function/geosparql/> ",
	"PREFIX units: <http://www.opengis.net/def/uom/OGC/1.0/> ",
	"PREFIX sf: <http://www.opengis.net/ont/sf#> ",
	"PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> ",
	"SELECT ?subject ?gml  ?dimensions ?feature_Class ",
	"FROM NAMED <http://localhost:8080/marmotta/context/v2/concise_feature> ",
	"FROM  <http://localhost:8080/marmotta/context/v2/trans_roadsegment> ",
	"WHERE { ",
	"<http://data.usgs.gov/lod/topo/trans_roadsegment/Trans_RoadSegment.15366> <http://www.opengis.net/ont/geosparql#hasGeometry> ?geom1 . ",
	"?geom1 <http://www.opengis.net/ont/geosparql#asWKT> ?gWKT1 . ",
	"GRAPH ?g{ ",
	"?subject <http://www.opengis.net/ont/geosparql#hasGeometry> ?geom . ",
	"?subject <http://data.usgs.gov/lod/topo/feature_Class> ?feature_Class . ",
	"?geom <http://www.opengis.net/ont/geosparql#asWKT> ?gWKT . ",
	"?geom <http://www.opengis.net/ont/geosparql#coordinateDimension> ?dimensions . ",
	"?geom <http://www.opengis.net/ont/geosparql#asGML> ?gml . ",
	"} ",
	"BIND( geof:buffer(?gWKT1, .0001 , units:degree) AS ?area ) ",
	"FILTER( geof:sfWithin(?gWKT, ?area) && ?g = <http://localhost:8080/marmotta/context/v2/concise_feature>) ",
	"} "
	]

