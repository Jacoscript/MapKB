    "IRI_Search_1":
        ["SELECT ?subject ?gml ?dimensions INPUT_STRING_1 ",
		"WHERE { GRAPH ?g { ",
			"?subject <http://www.opengis.net/ont/geosparql#hasGeometry> ?geom . INPUT_STRING_2 " ,
			"?geom <http://www.opengis.net/ont/geosparql#asGML> ?gml . ",
			"?geom <http://www.opengis.net/ont/geosparql#coordinateDimension> ?dimensions . ",
		" } ",
		"FILTER regex( INPUT_STRING_3 , \"INPUT_STRING_4\")  } "],
